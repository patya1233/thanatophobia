	Callback = function()
-- Function to create an ESP box
local function createESPBox(part, size)
    -- Remove existing ESP box if it exists
    if part:FindFirstChild("ESPBox") then
        part.ESPBox:Destroy()
    end

    -- Create a new ESP box
